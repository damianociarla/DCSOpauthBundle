    "license": "MIT",
    "homepage": "https://github.com/damianociarla/DCSOpauthBundle",
    "authors": [
        {
            "name": "Damiano Ciarla",
            "email": "damiano.ciarla@gmail.com",
            "homepage": "http://www.damianociarla.it/"
        }
    ],
    "require": {
        "php": ">=5.3",
        "symfony/symfony": "2.3.*",
        "opauth/opauth": "0.4.4"
    },
    "autoload": {
        "psr-0": { "DCS\\OpauthBundle": "" }
    },
    "target-dir": "DCS/OpauthBundle"
